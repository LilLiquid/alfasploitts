    <div class="background-container">
        <script>
            const container = document.querySelector('.background-container');
            const imageUrl = "https://media.discordapp.net/attachments/1317943491820519476/1324488986441289778/png-clipart-candy-cane-ribbon-candy-taffy-cartoon-s-of-candy-food-cartoon-removebg-preview.png?ex=677855fe&is=6777047e&hm=7abc312322d506d8dcca9e6d106366b66a9a79e434853a43a75b3889a6161bd1&=&format=webp&quality=lossless&width=662&height=588";

            for (let i = 0; i < 50; i++) {
                const img = document.createElement('img');
                img.src = imageUrl;
                img.classList.add('falling-image');
                img.style.left = Math.random() * 100 + 'vw';
                img.style.animationDelay = Math.random() * 10 + 's';
                img.style.animationDuration = 5 + Math.random() * 5 + 's';
                container.appendChild(img);
            }
        </script>
